{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
      {
        "source": "/henna-album/assets/:path*",
        "destination": "/assets/:path*"
      },
      {
        "source": "/henna-album/:path*",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": false,
    "trailingSlash": false
}
